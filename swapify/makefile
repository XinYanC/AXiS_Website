# Swapify Makefile
# Run from swapify/ directory.

# install dependencies
install:
	npm install

# start the dev server (http://localhost:5173)
dev:
	npm run dev

# prod build (dist/)
build:
	npm run build

# run ESLint
lint:
	npm run lint

# remove node_modules and dist
clean:
	rm -rf node_modules dist
